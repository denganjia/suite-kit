{
	"compilerOptions": {
		"outDir": "dist",
		"target": "ESNext",
		"module": "esnext",
		"baseUrl": ".",
		"sourceMap": false,
		"moduleResolution": "node",
		"allowJs": false,
		"strict": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"removeComments": false,
		"rootDir": ".",
		"types": [],
		"jsx": "preserve",
		"skipLibCheck": true
	}
}
